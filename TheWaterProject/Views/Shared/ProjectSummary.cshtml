@{
}

@model Project

<div class="card m-1 p-1">
    <h4>
        @Model.ProjectName
        <span class="badge roundedcard-header-pills bg-primary text-white float-end"><small>@Model.ProjectImpact served</small></span>
    </h4>
    <ul>
        <li>Project Type @Model.ProjectType</li>
        <li>Regional Program @Model.ProgramName</li>
        <li>Installation Date @Model.ProjectInstallation</li>
        <li>Project Phase @Model.ProjectPhase</li>
    </ul>
</div>